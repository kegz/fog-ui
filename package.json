{
  "name": "fog-ui",
  "version": "0.1.5",
  "private": false,
  "type": "module",
  "description": "fog-ui provides lightweight, composable UI primitives that reduce complexity without over-prescribing design.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kegz/fog-ui.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.3.4",
    "@mui/system": "^7.3.3",
    "@reduxjs/toolkit": "^2.8.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "axios": "^1.11.0",
    "html2pdf.js": "^0.14.0",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.548.0",
    "papaparse": "^5.5.3",
    "react": ">=18",
    "react-dom": ">=18",
    "react-hook-form": "^7.61.1",
    "react-i18next": "^15.6.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.7.1",
    "recharts": "^3.1.0",
    "redux": "^5.0.1",
    "swr": "^2.3.4",
    "web-vitals": "^2.1.4",
    "zod": "^4.0.14"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build:types && vite build",
    "build:types": "tsc -p tsconfig.build.json",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run lint && npm run build",
    "drypublishOnly": "npm publish --dry-run",
    "versionBump": "npm version patch",
    "publish:public": "npm publish --access public",
    "postpublish": "git push --follow-tags"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "jest": "^27.5.1",
    "jsdom": "^22.1.0",
    "path": "^0.12.7",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.52.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  }
}

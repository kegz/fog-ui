{
  "name": "fog-ui",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "description": "fog-ui provides lightweight, composable UI primitives that reduce complexity without over-prescribing design.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kegz/fog-ui.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run build:types && vite build",
    "build:types": "tsc -p tsconfig.build.json",
    "lint": "eslint .",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "path": "^0.12.7",
    "vite": "^7.3.1"
  }
}
{
	"compilerOptions": {
		"target": "ES2020",
		"lib": [
			"ES2020",
			"DOM",
			"DOM.Iterable"
		],
		"jsx": "react-jsx",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"skipLibCheck": true,
		"noEmit": true,
		"types": [
			"vite/client"
		]
	},
	"files": [
		"node_modules"
	],
	"include": [
		"src/**/*"
	],
	"exclude": [
		"node_modules",
		"dist"
	]
}